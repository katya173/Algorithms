def merge_sort(A: list, B: list) -> list:
    '''
    The function implements merge sorting with complexity O(n * log n)
    :param A: list
    :param B: list
    :return: list  
    '''
    C = [0] * (len(A) + len(B))
    i = j = k = 0
    while i < len(A) and j < len(B):
        if A[i] <= B[j]:
            C[k] = A[i]
            i += 1
            k += 1
        else:
            C[k] = B[j]
            j += 1
            k += 1
    while i < len(A):
        C[k] = A[i]
        i += 1
        k += 1
    while j < len(B):
        C[k] = B[j]
        j += 1
        k += 1
    return C
